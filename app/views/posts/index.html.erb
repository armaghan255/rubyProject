<h1>Posts#index</h1>

<div class="container">
<table border="2">

<tr>
  <th>Title</th>
  <th colspan="3">Description</th>
  <th>Has Attachment?</th>
</tr>
<% if @posts%>
<% @posts.each do |post|%>
  <tr>
    <td><%= post.title %></td>
    <td colspan="3"><%= post.description %></td>
    <td ><%= post.has_attachment  %></td>
  </tr>
<%end%>
<%end%>
</table>
</div>
